{
  "name": "e-shop-api",
  "version": "0.0.1",
  "scripts": {
    "init": "npm run db:build && npm run api:command create-database && npm run db:migrate && npm run api:build && npm run api:command generate-encryption-key",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "npm run db:build && npm run db:migrate && npm run api:build",
    "start": "npm run api:start:prod",
    "db:build": "prisma generate --schema=./src/database/schema.prisma",
    "db:build:dev": "prisma generate --schema=./src/database/schema.prisma --watch",
    "db:migrate": "prisma migrate dev --schema=./src/database/schema.prisma",
    "db:browse": "prisma studio --schema=./src/database/schema.prisma",
    "api:command": "ts-node --project tsconfig.cli.json src/cli",
    "api:build": "rimraf build && nest build",
    "api:start:dev": "nest start --watch",
    "api:start:prod": "node build/main"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@prisma/client": "^4.9.0",
    "@types/bcrypt": "^5.0.0",
    "@types/dot-object": "^2.1.2",
    "@types/lodash": "^4.14.191",
    "bcrypt": "^5.1.0",
    "dot-object": "^2.1.4",
    "lodash": "^4.17.21",
    "nest-commander": "^3.4.0",
    "nestjs-prisma": "^0.20.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "prisma": "^4.9.0",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "main": "index.js",
  "repository": "https://github.com/andrei-kochetygov/e-shop-api.git",
  "author": "capslol <35397947+capslol@users.noreply.github.com>",
  "license": "MIT"
}
